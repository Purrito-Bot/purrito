    "url": "git+https://github.com/djaustin/purrito-bot.git"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Daniel Austin, Matthew Beech",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/djaustin/purrito-bot/issues"
  },
  "homepage": "https://github.com/djaustin/purrito-bot#readme",
  "dependencies": {
    "@ensemblebr/dice": "^3.0.0",
    "discord.js": "^12.4.1",
    "dotenv": "^10.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^15.12.5",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
